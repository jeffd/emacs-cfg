(require 'scheme-lookup)

(defvar scheme-lookup-scheme48-base
  (if (file-exists-p "/usr/share/doc/scheme48-doc/html/manual.html")
      "file:///usr/share/doc/scheme48-doc/html/manual" ; Debian installs here
      "http://s48.org/1.3/manual/manual"))

(defun scheme-lookup-scheme48 (symbol-name chapter page)
  (scheme-lookup-tex2page scheme-lookup-scheme48-base chapter page))

(put 'scheme-lookup-scheme48
     'scheme-lookup-pretty-name
     "Scheme48 Manual")

(mapc (lambda (x)
